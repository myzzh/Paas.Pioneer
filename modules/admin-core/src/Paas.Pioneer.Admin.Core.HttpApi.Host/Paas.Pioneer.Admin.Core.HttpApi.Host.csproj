<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<PackageType>Paas.Pioneer.Admin.Core.HttpApi.Host</PackageType>
		<PackageId>Paas.Pioneer.Admin.Core.HttpApi.Host</PackageId>
