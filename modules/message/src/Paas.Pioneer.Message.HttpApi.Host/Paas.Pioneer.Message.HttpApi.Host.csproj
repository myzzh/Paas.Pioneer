<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<PackageType>Paas.Pioneer.Message.HttpApi.Host</PackageType>
		<PackageId>Paas.Pioneer.Message.HttpApi.Host</PackageId>
		<Title>Paas.Pioneer.Message.HttpApi.Host</Title>
		<Authors>Paas.Pioneer</Authors>
		<Description>Paas.Pioneer.Message.HttpApi.Host</Description>
		<TargetFramework>net6.0</TargetFramework>
		<IncludeBuildOutput>true</IncludeBuildOutput>
		<NoWarn>$(NoWarn);NU5128</NoWarn>
		<RootNamespace>$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
		<PreserveCompilationReferences>true</PreserveCompilationReferences>
		<UserSecretsId>Paas.Pioneer.Messages-4681b4fd-151f-4221-84a4-929d86723e4c</UserSecretsId>
	</PropertyGroup>

	<ItemGroup>
		<Content Remove="Configs\appconfig.json" />
		<Content Remove="Configs\dbconfig.json" />
		<Content Remove="Configs\jwtconfig.json" />
		<Content Remove="Configs\uploadconfig.json" />
		<Content Remove="NLog.config" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Volo.Abp.AspNetCore.MultiTenancy" Version="5.3.0" />
		<PackageReference Include="Volo.Abp.Autofac" Version="5.3.0" />
		<PackageReference Include="Volo.Abp.AspNetCore.Authentication.JwtBearer" Version="5.3.0" />
		<PackageReference Include="Paas.Pioneer.Knife4jUI.Swagger" Version="5.3.1" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Paas.Pioneer.Message.Application\Paas.Pioneer.Message.Application.csproj" />
		<ProjectReference Include="..\Paas.Pioneer.Message.EntityFrameworkCore\Paas.Pioneer.Message.EntityFrameworkCore.csproj" />
		<ProjectReference Include="..\Paas.Pioneer.Message.HttpApi\Paas.Pioneer.Message.HttpApi.csproj" />
	</ItemGroup>

	<ItemGroup>
		<None Include="Configs\appconfig.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Include="Configs\dbconfig.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Include="Configs\jwtconfig.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Include="Configs\uploadconfig.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Include="NLog.config">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
</Project>
